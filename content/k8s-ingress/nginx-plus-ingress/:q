apiVersion: v1
kind: Service
metadata:
  labels:
    cis.f5.com/as3-tenant: nginx-ingress
    cis.f5.com/as3-app: nginx-ingress-app
    cis.f5.com/as3-pool: nginx-ingress-pool
  name: nginx-ingress-svc
  namespace: nginx-ingress
spec:
  ports:
  - name: http
    port: 80
    protocol: TCP
    targetPort: 80
  selector:
    app: nginx-ingress
  sessionAffinity: None
  type: ClusterIP
